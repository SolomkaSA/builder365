{
	"name": "builder365solution",
	"displayName": "builder365solution",
	"description": "",
	"version": "0.0.2",
	"publisher": "SolomkaSA",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:builder365solution.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "builder365solution.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run ClearOutFolder && npm run compile",
		"compile": "tsc -p ./ && npm run PsSCriptsCopy && npm run TemplateCopy",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"ClearOutFolder": "rimraf ./out",
		"PsSCriptsCopy": "@powershell copy  './src/PsScripts' './out/PsScripts' –Recurse",
		"TemplateCopy": "@powershell copy  './src/PreparedTemplates' './out/PreparedTemplates' –Recurse"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}